        <activity
            android:name=".touchevent.EventDispatchActivity"
            android:launchMode="standard">
            <intent-filter>
                <action android:name="android.intent.action.Atext"/>
                <action android:name="android.intent.action.Aqtext"/>

                <category android:name="android.intent.category.DEFAULT"/>
                <category android:name="android.intent.category.aa"/>
            </intent-filter>
        </activity>
        <activity android:name=".hashmap.HashMapActivity"/>
        <activity android:name=".mvp.activity.ViewActivity"/>
        <activity android:name=".threadpool.ThreadPoolsActivity"/>
        <activity android:name=".AntimatorsActivity"/>
